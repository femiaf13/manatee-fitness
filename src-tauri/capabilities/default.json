{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "store:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.openfoodfacts.org"
        }
      ]
    },
    "os:default"
  ]
}