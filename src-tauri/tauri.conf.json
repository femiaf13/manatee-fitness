{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "manatee-fitness",
  "version": "0.1.0",
  "identifier": "fit.manatee-fitness.app",
  "build": {
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/manatee-fitness/browser"
  },
  "app": {
    "windows": [
      {
        "title": "Manatee Fitness",
        "width": 400,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
